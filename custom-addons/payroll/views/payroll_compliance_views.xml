<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_payroll_compliance_tree" model="ir.ui.view">
        <field name="name">payroll.compliance.tree</field>
        <field name="model">payroll.compliance</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="country_id"/>
                <field name="tax_rate"/>
                <field name="currency_id"/>
            </tree>
        </field>
    </record>

    <record id="view_payroll_compliance_form" model="ir.ui.view">
        <field name="name">payroll.compliance.form</field>
        <field name="model">payroll.compliance</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="country_id"/>
                        <field name="tax_rate"/>
                        <field name="currency_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <menuitem id="payroll_menu_compliance" name="Payroll Compliance" parent="payroll_menu_root"/>

    <act_window id="action_payroll_compliance"
        name="Payroll Compliance"
        res_model="payroll.compliance"
        view_mode="tree,form"
        menu_id="payroll_menu_compliance"/>
</odoo>
